{
  "name": "nodeTest",
  "version": "1.0.0",
  "scripts": {
    "build": "mkdir build && cd client && npm i && npm run build && mv build/* ../build && rmdir build && cd ../",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "start": "node server.js"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "node": "^8.17.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
